algoritmo "antecessor e posterior"
var
n:inteiro
antecessor:inteiro
posterior: inteiro
inicio
     escreva("escreva um numero para calcular o antecessor e posterior: ")
     leia(n)
     antecessor <- n-1
     posterior <- n+1
     escreva ("O número antecessor de ", n, " é ", antecessor," e o posterior é ",posterior)
     
fimalgoritmo